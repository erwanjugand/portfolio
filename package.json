    "preview": "nuxt preview",
    "check:prettier": "prettier --check . --ignore-path .gitignore",
    "check:eslint": "eslint --ext .ts,.js,.vue .",
    "check:stylelint": "stylelint **/*.{scss,vue}",
    "check:ts": "vue-tsc --noEmit",
    "check:all": "yarn check:prettier && yarn check:eslint && yarn check:stylelint && yarn check:ts",
    "check:ci": "yarn nuxi prepare && yarn check:all",
    "fix:prettier": "prettier --loglevel warn --write . --ignore-path .gitignore",
    "fix:eslint": "yarn check:eslint --fix",
    "fix:stylelint": "yarn check:stylelint --fix",
    "fix:svg": "svgo public --recursive --quiet",
    "fix:image": "sh parse-image.sh",
    "fix:all": "yarn fix:prettier && yarn fix:eslint && yarn fix:stylelint && yarn fix:svg"
  },
